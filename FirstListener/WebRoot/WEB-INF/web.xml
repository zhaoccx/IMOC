<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee   http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
 <context-param>
  <param-name>initParam</param-name>
  <param-value>imooc</param-value>
 </context-param>
 <listener>
  <listener-class>com.imooc.listener.FirstListener</listener-class>
 </listener>
 <listener>
  <listener-class>com.imooc.listener.MyHttpSessionListener</listener-class>
 </listener>
 <listener>
  <listener-class>com.imooc.listener.MyServletRequestListener</listener-class>
 </listener>
 <listener>
  <listener-class>com.imooc.listener.MyServletContextAttributeListener</listener-class>
 </listener>
 <listener>
  <listener-class>com.imooc.listener.MyHttpSessionAttributeListener</listener-class>
 </listener>
 <listener>
  <listener-class>com.imooc.listener.MyServletRequestAttributeListener</listener-class>
 </listener>
 <session-config>
  <session-timeout>0</session-timeout>
 </session-config>
 <welcome-file-list>
  <welcome-file>index.jsp</welcome-file>
 </welcome-file-list>
 <login-config>
  <auth-method>BASIC</auth-method>
 </login-config>
</web-app>
